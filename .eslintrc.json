{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "_": true
  },
  "rules": {
    "comma-dangle": [0],
    "max-len": [0],
    "camelcase": [0],
    "quotes": [0],
    "prefer-arrow-callback": [0],
    "new-cap": [0],
    "no-underscore-dangle": [0],
    "arrow-body-style": [0],
    "no-param-reassign": [0],
    "no-console": [0],
    "eqeqeq": [1]
  },
  "extends": "airbnb-base"
}
